<%# default partial to display solr document fields in catalog show view -%>
<dl class="defList">
  <% document_show_fields.each do |solr_fname| -%>
    <% if document[solr_fname] %>
	<dt class="blacklight-<%= solr_fname.parameterize %>"><%= render_document_show_field_label solr_fname %></dt>
	<dd class="blacklight-<%= solr_fname.parameterize %>"><%= render_document_show_field_value document[solr_fname], solr_fname %></dd>
    <% end -%>
  <% end -%>
</dl>
