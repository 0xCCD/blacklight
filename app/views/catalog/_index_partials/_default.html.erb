<%# default partial to display solr document fields in catalog index view -%>
<dl class="defList">
  
  <% index_field_names.each do |solr_fname| -%>
    <% if document.get solr_fname %>
	<dt class="blacklight-<%= solr_fname.parameterize %>"><%= render_index_field_label solr_fname %></dt>
	<dd class="blacklight-<%= solr_fname.parameterize %>"><%= render_index_field_value document.get(solr_fname), solr_fname %></dd>
    <% end -%>
  <% end -%>

</dl>
